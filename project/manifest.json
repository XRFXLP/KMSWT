{
    "manifest_version": 2,
    "name": "WikipediaVisualizer",
    "version": "1.0",
    "content_scripts" : [
    {
        "matches" : [
            "https://en.wikipedia.org/wiki/*"            
        ],
        "js" : ["content.js", "vis-network.js"]
    }  
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["<all_urls>", "tabs", "background"]
}