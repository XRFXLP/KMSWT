{
    "manifest_version": 2,
    "name": "WikipediaVisualizer",
    "version": "1.0",
    "content_scripts" : [
    {
        "matches" : [
            "https://en.wikipedia.org/wiki/*"            
        ],
        "js" : ["content.js", "vis-network.js"]
    }  
    ],
    "background": {
        "page": "background.html"
    },
    "permissions": ["<all_urls>"]
}